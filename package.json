  "scripts": {
    "babel": "babel components index.js --out-dir lib",
    "clean-build": "rm -rf _site _data && ./scripts/build-website.js",
    "start": "npm run clean-build && RUN_ENV=WEBSITE dora -p 8001 --plugins webpack?disableNpmInstall,hmr",
    "copy-html": "cp ./index.html ./_site/index.html && cp ./kitchen-sink.html ./_site/kitchen-sink.html",
    "site": "npm run clean-build && RUN_ENV=WEBSITE atool-build --no-compress --devtool=#sourcemap -o ./_site && npm run copy-html",
    "deploy": "rm -rf node_modules && node scripts/install.js && npm run just-deploy",
    "just-deploy": "npm run site && node scripts/deploy.js",
    "lint": "npm run srclint && npm run demolint && npm run lesshint",
    "srclint": "eslint components test site index.js --ext '.js,.jsx'",
    "demolint": "RUN_ENV=DEMO eslint components/*/demo/*.md --ext '.md'",
    "lesshint": "lesshint style/ components/",
    "pub": "sh ./scripts/publish.sh",
    "beta": "sh ./scripts/publish.sh --tag beta",
    "authors": "git log --format='%aN <%aE>' | sort -u | grep -v 'users.noreply.github.com' > AUTHORS.txt"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  },
  "pre-commit": [
    "lint"
  ]
}
